@model Data_Access.Custom_Models.AdminDashboardCm;

@{
    ViewData["Title"] = "Admin Dashboard";
}

<partial name="_AdminMainHeader" />

<div class="p-sm-2 p-md-5 mt-4 mt-sm-0">
    <div class="tab-content" id="myTabContent">
        <div class=" tab-pane fade show active" id="home-tab-pane" role="tabpanel" aria-labelledby="home-tab"
             tabindex="0">
        </div>

        <div class="tab-pane fade" id="provider-location-tab-pane" role="tabpanel"
             aria-labelledby="provider-location-tab" tabindex="0">
            <div class="container-fluid">
            </div>
        </div>

        <div class="tab-pane fade" id="Admin-profile-tab-pane" role="tabpanel" aria-labelledby="profile-tab" tabindex="0">
            <div class="container-fluid">
            </div>
        </div>

        <div class="tab-pane fade" id="Provider-tab-pane" role="tabpanel" aria-labelledby="Providers-tab"
             tabindex="0">
            <div class="container-fluid">
            </div>
        </div>

        <div class="tab-pane fade" id="Scheduling-tab-pane" role="tabpanel" aria-labelledby="Scheduling-tab"
             tabindex="0">
            <div class="container-fluid">
            </div>
        </div>

        <div class="tab-pane fade" id="Invoicing-tab-pane" role="tabpanel" aria-labelledby="Invoicing-tab"
             tabindex="0">
            <div class="container-fluid">
            </div>
        </div>

        <div class="tab-pane fade" id="Partners-tab-pane" role="tabpanel" aria-labelledby="Partners-tab"
             tabindex="0">
            <div class="container-fluid">
            </div>
        </div>

        <div class="tab-pane fade" id="Account-tab-pane" role="tabpanel" aria-labelledby="Account-tab" tabindex="0">
            <div class="container-fluid">
            </div>
        </div>

        <div class="tab-pane fade" id="User-tab-pane" role="tabpanel" aria-labelledby="User-tab" tabindex="0">
            <div class="container-fluid">
            </div>
        </div>

        <div class="tab-pane fade" id="Search-tab-pane" role="tabpanel" aria-labelledby="Search-tab" tabindex="0">
            <div class="container-fluid">
            </div>
        </div>

        <div class="tab-pane fade" id="Email-tab-pane" role="tabpanel" aria-labelledby="Email-tab" tabindex="0">
            <div class="container-fluid">
            </div>
        </div>

        <div class="tab-pane fade" id="SMS-tab-pane" role="tabpanel" aria-labelledby="SMS-tab" tabindex="0">
            <div class="container-fluid">
            </div>
        </div>

        <div class="tab-pane fade" id="Patient-tab-pane" role="tabpanel" aria-labelledby="Patient-tab" tabindex="0">
            <div class="container-fluid">
            </div>
        </div>

        <div class="tab-pane fade" id="Block-tab-pane" role="tabpanel" aria-labelledby="Block-tab" tabindex="0">
            <div class="container-fluid">
            </div>
        </div>

    </div>
</div>

<div id="showCaseModal"></div>

<div id="loader-container" class="loader-container" style="z-index:150">
    <div id="loader" class="loader" style="z-index:150"></div>
</div>

<div class="backdrop" style="z-index:140"></div>

<div id="chatContainer"></div>

<script src="~/js/signalr/dist/browser/signalr.js"></script>
<script src="~/js/signalr/dist/browser/signalr.min.js"></script>
<script src="~/js/chat.js"></script>

<script>
    document.addEventListener('DOMContentLoaded', function () {
        GetDashboard();
        setupSignalRConnection();
    });
</script>